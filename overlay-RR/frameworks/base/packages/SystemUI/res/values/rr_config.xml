<!-- These resources are around just to allow their values to be customized
     for different hardware and product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- On-The-Go -->
    <integer name="quick_settings_onthego_dialog_short_timeout">3000</integer>
    <integer name="quick_settings_onthego_dialog_long_timeout">6000</integer>


    <!-- Whether to show VoLTE icon in the status bar -->
    <bool name="config_display_volte">true</bool>

    <!-- Whether to use the mask for the quicksettings icons-->
    <bool name="config_useMaskForQs">true</bool>

    <!-- FPSInfoService FPS node file path -->
    <string name="config_fpsInfoSysNode" translatable="false"></string>

    <integer name="quick_settings_num_columns_land">4</integer>

    <!-- Enable face auth only when swiping security view -->
    <bool name="config_faceAuthOnlyOnSecurityView">false</bool>

    <!-- Whether to disable assist hint on lockscreen -->
    <bool name="config_disableAssistHintOnLockscreen">false</bool>

    <!-- Allow devices override audio panel location to the left side -->
    <bool name="config_audioPanelOnLeftSide">false</bool>

    <bool name="qs_status_bar_hidden">false</bool>

    <!-- Burn in protection -->
    <bool name="config_statusBarBurnInProtection">true</bool>
    <integer name="config_shift_interval">30</integer>

    <integer name="config_maxVisibleStatusIconContainer">6</integer>

    <!-- Allow devices to use unordinary cutout overlays -->
    <bool name="config_customCutout">false</bool>

</resources>
